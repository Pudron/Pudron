mathDLL=DLL("math.dll");
sqrt=mathDLL.getFunc("msqrt");##开平方
pow=mathDLL.getFunc("mpow");##次方
acos=mathDLL.getFunc("macos");##反余弦
asin=mathDLL.getFunc("masin");##反正弦
atan=mathDLL.getFunc("matan");##反正切
atan2=mathDLL.getFunc("matan2");## arg1/arg2 的反正切
cos=mathDLL.getFunc("mcos");##余弦
cosh=mathDLL.getFunc("mcosh");##双曲余弦
sin=mathDLL.getFunc("msin");##正弦
sinh=mathDLL.getFunc("msinh");##双曲正弦
tan=mathDLL.getFunc("mtan");##正切
tanh=mathDLL.getFunc("mtanh");##双曲正切
exp=mathDLL.getFunc("mexp");## e 的 arg1 次幂
log=mathDLL.getFunc("mlog");##对数
log10=mathDLL.getFunc("mlog10");##以10为低的对数
ceil=mathDLL.getFunc("mceil");##大于或等于 arg1 的最小的整数值
fabs=mathDLL.getFunc("mabs");##arg1的绝对值
floor=mathDLL.getFunc("mfloor");##小于或等于 arg1 的最大的整数值
fmod=mathDLL.getFunc("mmod");## arg1 除以 arg2 的余数