class string{
    #不允许有initstring(),以防无限递归#
    func operator+(val){
        str;
        str=copyVar();
        >>LOAD_VAL str;
        >>LOAD_VAL val;
        >>STR_FORMAT;
        >>LOAD_VAL str;
        >>RETURN;
    }
    func operator=(str){
        >>LOAD_VAL this;
        >>LOAD_VAL str;
        >>STR_COMPARE;
        >>RETURN;
    }
    func code(){
        return this[0];
    }
}
func print(text){
    str;
    str="\0";
    for(i,argVar){
        >>LOAD_VAL str;
        >>LOAD_VAL i;
        >>STR_FORMAT;
    }
    >>LOAD_VAL str;
    >>PRINT;
}
