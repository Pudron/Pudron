class DLL{
    dllptr;
    func initDLL(path){
        >>LOAD_VAR path;
        >>DLL_OPEN;
        >>STORE_VAL dllptr;
    }
    func destroyDLL(){
        >>LOAD_VAR dllptr;
        >>DLL_CLOSE;
    }
    func execute(type,name,arg){
        >>LOAD_VAR dllptr;
        >>LOAD_VAR type;
        >>LOAD_VAR name;
        >>LOAD_VAR arg;
        >>DLL_EXECUTE;
        >>RETURN;
    }
}