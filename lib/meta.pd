class meta{
    #没有类变量#
    func resizeVar(size){
        >>LOAD_VAR this;
        >>LOAD_VAR size;
        >>RESIZE_VAR;
    }
    func getVarCount(){
        >>LOAD_VAR this;
        >>GET_VARCOUNT;
        >>RETURN;
    }
    func getClass(){
        >>LOAD_VAR this;
        >>GET_CLASS;
        >>RETURN;
    }
    func copyVar(){
        >>LOAD_VAR this;
        >>COPY_OBJECT;
        >>RETURN;
    }
    func getVarBasis(){
        >>LOAD_VAR this;
        >>GET_VARBASIS;
        >>RETURN;
    }
    func setVarBasis(val){
        >>LOAD_VAR this;
        >>LOAD_VAR val;
        >>SET_VARBASIS;
    }
}
func exit(value){
    >>LOAD_VAR value;
    >>EXIT;
}
func range(a,b,c){
    result;
    if(argVar=1){
        a-=1;
        >>LOAD_CONST 0;
        >>LOAD_VAR a;
        >>LOAD_CONST 1;
        >>MAKE_RANGE;
        >>STORE_VAL result;
    }elif(argVar=2){
        >>LOAD_VAR a;
        >>LOAD_VAR b;
        >>LOAD_CONST 1;
        >>MAKE_RANGE;
        >>STORE_VAL result;
    }elif(argVar=3){
        >>LOAD_VAR a;
        >>LOAD_VAR b;
        >>LOAD_VAR c;
        >>MAKE_RANGE;
        >>STORE_VAL result;
    }
    return result;
}