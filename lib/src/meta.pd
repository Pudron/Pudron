class meta{
    #没有类变量#
    func resizeVar(size){
        >>LOAD_VAL this;
        >>LOAD_VAL size;
        >>RESIZE_VAR;
    }
    func getVarCount(){
        >>LOAD_VAL this;
        >>GET_VARCOUNT;
        >>RETURN;
    }
    func getClass(){
        >>LOAD_VAL this;
        >>GET_CLASS;
        >>RETURN;
    }
    func copyVar(){
        >>LOAD_VAL this;
        >>COPY_OBJECT;
        >>RETURN;
    }
    func getVarBasis(){
        >>LOAD_VAL this;
        >>GET_VARBASIS;
        >>RETURN;
    }
    func setVarBasis(val){
        >>LOAD_VAL this;
        >>LOAD_VAL val;
        >>SET_VARBASIS;
    }
}
func exit(value){
    >>LOAD_VAL value;
    >>EXIT;
}
func range(a,b,c){
    result;
    if(argVar.getVarCount()=1){
        a-=1;
        >>LOAD_CONST 0;
        >>LOAD_VAL a;
        >>LOAD_CONST 1;
        >>MAKE_RANGE;
        >>STORE_VAL result;
    }elif(argVar.getVarCount()=2){
        >>LOAD_VAL a;
        >>LOAD_VAL b;
        >>LOAD_CONST 1;
        >>MAKE_RANGE;
        >>STORE_VAL result;
    }elif(argVar.getVarCount()=3){
        >>LOAD_VAL a;
        >>LOAD_VAL b;
        >>LOAD_VAL c;
        >>MAKE_RANGE;
        >>STORE_VAL result;
    }
    return result;
}