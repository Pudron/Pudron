class List{
    count=0;
    func initList(){
        resizeVar(getVarCount()+argVar);
        count=argVar;
        for(i,range(argVar)){
            this[i]=argVar[i];
        }
    }
    func add(element){
        resizeVar(getVarCount()+1);
        this[count]=element;
        count+=1;
    }
    func del(num){
        resizeVar(getVarCount()-num);
        count-=num;
    }
    func expand(size){
        resizeVar(getVarCount()+size);
        count+=size;
    }
    func connect(list){
        start;
        start=count;
        expand(list.count);
        for(i,range(list.count)){
            this[start+i]=list[i];
        }
    }
    func getArray(){
        array;
        array={};
        array.resizeVar(count);
        for(i,range(count)){
            array[i]=this[i];
        }
        return array;
    }
}